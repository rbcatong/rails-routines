<%=link_to 'Log In', login_path%>

<%=form_for @user do |f| %>

<%= f.label :username %>
<%= f.text_field :username%><p>

<%= f.label :full_name %>
<%= f.text_field :full_name%><p>

<%= f.label :password %>
<%= f.password_field :password%><p>

  <%= f.label :bio  %><br>
  <%= f.text_area :bio%><p>


<%= f.submit "Create User" %>
<%end%>
